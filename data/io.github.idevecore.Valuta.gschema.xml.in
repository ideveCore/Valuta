<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="valuta">
  <enum id="@app-id@.Window">
    <value nick="ecb" value="0"/>
    <value nick="google" value="1"/>
     <value nick="mi" value="2"/>
  </enum>
  <schema id="@app-id@" path="/io/github/idevecore/Valuta/">
    <key name="providers" enum="@app-id@.Window">
      <default>'ecb'</default>
    </key>
    <key name="width" type="i">
			<default>420</default>
		</key>
		<key name="height" type="i">
			<default>600</default>
		</key>
		<key name="is-maximized" type="b">
			<default>false</default>
		</key>
		<key name="is-fullscreen" type="b">
			<default>false</default>
		</key>
    <key type="s" name="src-currency">
        <default>'USD'</default>
    </key>
    <key type="s" name="dest-currency">
        <default>'EUR'</default>
    </key>
  </schema>
</schemalist>
